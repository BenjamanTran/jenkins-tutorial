# SSH Config for jump host
Host jump-host
    HostName 118.69.34.46
    Port 3333
    User zigexn
    IdentityFile ~/.ssh/newbie_id_rsa
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

Host remote-server
    HostName 10.1.1.195
    User newbie
    IdentityFile ~/.ssh/newbie_id_rsa
    ProxyJump jump-host
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
